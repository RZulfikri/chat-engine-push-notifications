{
  "rootDir": "../../",
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "coverageReporters": [
    "json",
    "text",
    "lcov",
    "clover"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/demo",
    "<rootDir>/node_modules/react-native/Libraries/react-native/"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/.idea",
    "<rootDir>/Libraries",
    "<rootDir>/coverage",
    "<rootDir>/docs",
    "/node_modules/"
  ],
  "notify": true,
  "preset": "react-native"
}