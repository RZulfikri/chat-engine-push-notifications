{
  "author": {
    "name": "Serhii Mamontov <sergey@pubnub.com>"
  },
  "dependencies": {
    "eventemitter2": "^4.1.2",
    "uuid": "3.x.x"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react-native": "^4.0.0",
    "esdoc": "^1.0.3",
    "esdoc-node": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.x.x",
    "eslint-plugin-import": "2.x.x",
    "eslint-plugin-jsx-a11y": "6.x.x",
    "eslint-plugin-react": "7.x.x",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-shell": "^0.6.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "react-native": "^0.48.4",
    "regenerator-runtime": "^0.11.0",
    "run-sequence": "^2.2.0"
  },
  "peerDependencies": {
    "chat-engine": "0.6.x",
    "react-native": "^0.48.4"
  },
  "description": "Open Chat Framework Push Notifications support plugin",
  "keywords": [
    "pubnub",
    "chat",
    "sdk",
    "realtime",
    "notifications"
  ],
  "license": "MIT",
  "main": "src/plugin.js",
  "name": "chat-engine-notifications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubnub/chat-engine-push-notifications.git"
  },
  "bugs": {
    "url": "https://github.com/pubnub/chat-engine-push-notifications/issues"
  },
  "homepage": "https://github.com/pubnub/chat-engine-push-notifications#readme",
  "version": "1.0.6",
  "scripts": {
    "test": "jest --ci",
    "unit_test": "jest --ci --forceExit --config=test/unit/jest-config.json --testPathPattern=/test/unit/",
    "integration_test": "jest --forceExit --config=test/integration/jest-config.json --testPathPattern=/test/integration/",
    "full_test": "jest --forceExit"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/",
      "<rootDir>/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/demo",
      "<rootDir>/node_modules/react-native/Libraries/react-native/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.idea",
      "<rootDir>/Libraries",
      "<rootDir>/coverage",
      "<rootDir>/docs",
      "<rootDir>/node_modules/"
    ],
    "notify": true,
    "preset": "react-native"
  }
}
